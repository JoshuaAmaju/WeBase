{
	"name": "webase",
	"version": "0.0.1",
	"description": "Graph database backed by IndexedDB",
	"module": "dist/webase.js",
	"main": "dist/webase.js",
	"browser": "dist/webase.umd.js",
	"typings": "dist/webase.d.ts",
	"scripts": {
		"start": "rollup -c -w",
		"clean": "rimraf ./dist",
		"prepare": "pnpm run build",
