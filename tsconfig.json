{
    "compilerOptions": {
        // "noEmit": true,
        "strict": true,
        "outDir": "dist",
        "lib": ["ESNext"],
        "target": "ES2015",
        "module": "ESNext",
        "declaration": true,
        "noEmitOnError": true,
        "noImplicitAny": true,
        // "stripInternal": true,
        "noImplicitThis": true,
        "removeComments": true,
        "declarationMap": true,
        "emitDeclarationOnly": true
    },
    "include": ["src"],
    "exclude": ["node_modules"]
}